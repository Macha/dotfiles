set tabstop=4
set shiftwidth=4
set softtabstop=4
set textwidth=79
set noexpandtab
set autoindent
set nocompatible
set backspace=indent,eol,start
colors slate
filetype plugin on
filetype plugin indent on
syntax on
set nu!
set backupdir=~/.vim/backup,/tmp
let g:user_zen_expandabbr_key = '<c-e>'
let g:use_zen_complete_tag = 1
